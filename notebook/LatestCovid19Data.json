					"\r\n",
					"#connect to raw zone\r\n",
					"#account_name = \"azurediyauedls\"\r\n",
					"#container_name = \"raw\"\r\n",
					"#relative_path = path\r\n",
					"adls_path = 'abfss://%s@%s.dfs.core.windows.net/%s' % (sourcezone, account_name, sourcepath)"
